                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Canvas HorizontalAlignment="Left" VerticalAlignment="Center" Margin="5">
                            <Ellipse Name="OuterRing" Width="60" Height="60" StrokeDashArray="100"
